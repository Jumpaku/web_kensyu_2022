{
  "name": "tetris",
  "version": "1.0.0",
  "description": "tetris for web-front kadai 3",
  "private": true,
  "scripts": {
    "build": "run-s clean build:html build:js",
    "clean": "rm -rf ./dist/*",
    "serve": "cp -r dist/html/* public/ && cp dist/main.bundle.js public/main.js && cp dist/main.bundle.js.map public/main.bundle.js.map && http-server public -d -c1",
    "deploy": "cp -r dist/html/* public/ && cp dist/main.js public/main.js",
    "build:html": "cp -r src/html dist/",
    "build:js": "run-s build:js:compile build:js:bundle build:js:babel build:js:minify",
    "build:js:compile": "tsc",
    "build:js:bundle": "rollup --sourcemap --format es --file dist/main.bundle.js dist/js/main.js",
    "build:js:babel": "babel --out-file dist/main.babel.js dist/main.bundle.js",
    "build:js:minify": "terser --compress --mangle --output dist/main.min.js dist/main.babel.js",
    "test": "echo 'test not implemented'"
  },
  "author": "tomohiko-ito-10antz",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "browserify": "^17.0.0",
    "http-server": "^14.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "terser": "^5.6.0",
    "typescript": "^4.1.5"
  }
}
